<h1>goodfoodhunting</h1>

<!-- this hides new dishes link for those not signed in.  If you want to hide a button when logged out, add logged_in? to all places it could possibly be reached-->
<% if logged_in?%>
  <a href="/dishes/new">new dish!</a>
  <%end%>

<!-- <a href="/dishes/new">new dish!</a> -->

<% @dishes.each do |dish|  %>
  <h2><%= dish.name %></h2>
  <img src="<%=dish.image_url%>" alt="">
  <a href="/dishes/<%=dish.id%>">details</a>


<%end%>
